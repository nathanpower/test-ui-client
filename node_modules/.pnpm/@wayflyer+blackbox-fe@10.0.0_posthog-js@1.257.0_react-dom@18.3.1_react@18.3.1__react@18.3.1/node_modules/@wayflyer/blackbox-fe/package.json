{
  "name": "@wayflyer/blackbox-fe",
  "version": "10.0.0",
  "description": "Frontend observability package",
  "source": "src/index.ts",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js"
  },
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist",
    "!dist/**/*.tsbuildinfo"
  ],
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@datadog/browser-core": "^4.50.0",
    "@datadog/browser-logs": "^4.50.0",
    "@datadog/browser-rum": "^4.50.0",
    "path-to-regexp": "^6.3.0",
    "change-case": "^5.4.4",
    "js-cookie": "^3.0.5",
    "react-error-boundary": "^4.0.11"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@types/js-cookie": "^3.0.6",
    "axios": "^1.8.4",
    "posthog-js": "^1.236.1",
    "@wayflyer/vitest-jest-dom": "^1.1.0"
  },
  "peerDependencies": {
    "react": "^18.x",
    "react-dom": "^18.x",
    "posthog-js": "^1.236.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wayflyer/fefo.git",
    "directory": "packages/blackbox-fe"
  },
  "keywords": [
    "datadog",
    "posthog",
    "analytics",
    "observability",
    "frontend"
  ],
  "author": "Reggie Morgan",
  "license": "ISC",
  "scripts": {
    "clean": "rm -rf dist && rm -rf node_modules",
    "check-types": "tsc -b",
    "build": "tsc --project tsconfig.prod.json",
    "test": "vitest",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --max-warnings 0 --cache && pnpm format-check",
    "link": "node ../../tools/scripts/link-local.mjs",
    "unlink": "node ../../tools/scripts/unlink-local.mjs",
    "version": "npm version",
    "format": "prettier --write 'src/**/*.{js,ts,tsx}'",
    "format-check": "prettier --check 'src/**/*.{js,ts,tsx}'"
  }
}