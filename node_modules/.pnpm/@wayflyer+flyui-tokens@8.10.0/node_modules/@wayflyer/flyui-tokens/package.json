{
  "name": "@wayflyer/flyui-tokens",
  "version": "8.10.0",
  "source": "src/index.ts",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js"
    },
    "./dev": {
      "import": "./dist/esm/index-dev.js",
      "default": "./dist/esm/index-dev.js"
    }
  },
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist",
    "!dist/**/*.tsbuildinfo"
  ],
  "engines": {
    "node": ">=10"
  },
  "generators": "./generators.json",
  "executors": "./executors.json",
  "repository": {
    "type": "git",
    "url": "https://github.com/wayflyer/fefo.git",
    "directory": "packages/flyui-tokens"
  },
  "devDependencies": {
    "@nx/devkit": "^16.7.4",
    "@swc-node/register": "^1.10.10",
    "@swc/core": "1.12.9",
    "@testing-library/jest-dom": "^5.16.4",
    "hex-to-hsl": "^1.0.2",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "tsx": "^4.19.0",
    "vite": "7.0.2",
    "vitest": "^3.2.4",
    "vitest-canvas-mock": "^0.3.3",
    "@wayflyer/vitest-jest-dom": "^1.1.0"
  },
  "scripts": {
    "clean": "rm -rf dist && rm -rf node_modules",
    "check-types": "tsc -b",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --max-warnings 0 --cache && prettier 'src/**/*.{js,ts,tsx}'",
    "lint:format": "pnpm lint --fix",
    "test": "echo 'No tests specified!'",
    "link": "node ../../tools/scripts/link-local.mjs",
    "unlink": "node ../../tools/scripts/unlink-local.mjs",
    "generate-tokens": "tsx ./src/scripts/build-colors.ts && tsx ./src/scripts/build-semantic.ts && prettier 'src/**/*.{js,ts,tsx}' --write",
    "generate-custom-properties": "tsx ./src/scripts/build-custom-properties.ts",
    "build": "pnpm generate-tokens && pnpm generate-custom-properties && vite build && pnpm build-types",
    "build-types": "tsc --project tsconfig.prod.json && tsc-alias"
  }
}