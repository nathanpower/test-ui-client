{
  "name": "@wayflyer/flyui",
  "version": "191.2.2",
  "description": "Core component library for Wayflyer Design System",
  "license": "UNLICENSED",
  "author": "Nathan Power <nathan.power@wayflyer.com>",
  "repository": "git://github.com/wayflyer/fefo.git",
  "module": "dist/esm/index.js",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "types": "dist/esm/index.d.ts",
  "source": "src/index.ts",
  "engines": {
    "node": ">=10"
  },
  "files": [
    "dist",
    "!dist/**/*.tsbuildinfo"
  ],
  "dependencies": {
    "@crello/react-lottie": "^0.0.11",
    "@floating-ui/react": "^0.26.12",
    "@internationalized/date": "^3.8.1",
    "@juggle/resize-observer": "^3.4.0",
    "@reach/rect": "^0.18.0",
    "copy-to-clipboard": "^3.3.3",
    "dayzed": "^3.2.3",
    "lodash.debounce": "^4.0.8",
    "lodash.isobject": "^3.0.2",
    "lodash.partition": "^4.6.0",
    "lodash.uniqueid": "^4.0.1",
    "match-sorter": "^6.3.1",
    "react-aria": "^3.40.0",
    "react-aria-components": "^1.9.0",
    "react-container-dimensions": "^1.4.1",
    "react-dropzone": "^14.2.3",
    "react-highlight-words": "^0.20.0",
    "react-input-mask": "^3.0.0-alpha.2",
    "react-intl": "^6.2.5",
    "react-markdown": "^8.0.4",
    "react-select": "^5.7.4",
    "react-swipeable": "^7.0.1",
    "react-tabs": "^6.0.0",
    "react-virtual": "^2.10.4",
    "remark-gfm": "^3.0.1",
    "remove-accents": "^0.4.4",
    "sonner": "^2.0.5",
    "stdnum": "1.4.10",
    "use-focus-visible": "^1.0.2",
    "@wayflyer/flyui-icons": "^1.3.3",
    "@wayflyer/flyui-tokens": "^8.9.0"
  },
  "devDependencies": {
    "@tanstack/react-query": "^5.17.9",
    "@testing-library/jest-dom": "^5.16.5",
    "framer-motion": "^11.2.11",
    "moment": "^2.29.2",
    "react-router": "^7.2.0",
    "react-router-dom": "^7.2.0",
    "tsc-alias": "^1.8.2",
    "@wayflyer/blackbox-fe": "^10.0.0",
    "@wayflyer/dev-utils": "^2.8.1",
    "@wayflyer/vitest-jest-dom": "^1.1.0"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.17.9",
    "framer-motion": "^11.2.11",
    "moment": "^2.29.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.22.0",
    "react-router-dom": "^7.2.0",
    "styled-components": "^5.3.5",
    "@wayflyer/blackbox-fe": "^10.0.0"
  },
  "nx": {
    "implicitDependencies": [
      "packages/blackbox-fe"
    ]
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "clean": "rm -rf dist && rm -rf node_modules",
    "watch-types": "tsc-watch --project tsconfig.prod.json",
    "build-types": "tsc --project tsconfig.prod.json && tsc-alias",
    "build": "vite build && pnpm build-types",
    "watch": "pnpm watch-types & vite build --mode watch",
    "analyze": "vite build --mode analyze-html && npx http-server dist/esm/bundle-stats -p 8082",
    "copy-public": "cp -R public/* dist",
    "check-types": "tsc -b && tsc-alias",
    "link": "node ../../tools/scripts/link-local.mjs",
    "unlink": "node ../../tools/scripts/unlink-local.mjs",
    "start": "pnpm compile-translations && pnpm storybook",
    "test": "vitest",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --max-warnings 0 --cache && pnpm format-check",
    "format": "prettier --write 'src/**/*.{js,ts,tsx}'",
    "format-check": "prettier --check 'src/**/*.{js,ts,tsx}'",
    "version": "npm version",
    "storybook": "STORYBOOK_DEV=true sb dev",
    "build-storybook": "NODE_OPTIONS='--max_old_space_size=16384' sb build -o ./build --loglevel=info --disable-telemetry",
    "test-coverage": "vitest run --coverage",
    "storybook-snapshot": "pnpm sb build && npx http-server ./build -p 9117 --proxy http://localhost:9117? --cors -s",
    "test-storybook": "NODE_OPTIONS='--max-old-space-size=8192 --openssl-legacy-provider' test-storybook --maxWorkers=8 --url http://0.0.0.0:9117 -c ./.storybook --coverage --stories-json || true",
    "gather-all-coverage": "pnpm test-coverage && cp coverage/storybook/coverage-storybook.json coverage && nyc report --reporter=lcov -t coverage --report-dir coverage/merged",
    "storybook-coverage-report": "start-server-and-test storybook-snapshot 9117 test-storybook && pnpm gather-all-coverage",
    "serve-coverage-report": "npx http-server coverage/merged/lcov-report -p 9118  --proxy http://localhost:9117? --cors",
    "extract-translations": "formatjs extract 'src/**/*.ts*' --ignore='**/*.d.ts' --format=crowdin --out-file public/lang/default.json",
    "compile-translations": "formatjs compile-folder --format=crowdin --ast public/lang compiled-lang",
    "chromatic": "chromatic --only-changed --skip '@(dep-mgmt/**|dependabot/**|l10n_crowdin_action)' --exit-zero-on-changes --trace-changed expanded --auto-accept-changes main"
  }
}